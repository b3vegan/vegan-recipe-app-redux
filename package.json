{
  "name": "vegan-recipe-app",
  "version": "0.1.0",
  "description": "An app to store vegan recipes",
  "main": "server.js",
  "scripts": {
    "start": "babel-node server.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/test_helper.js",
    "test:watch": "npm run test -- --watch",
    "postinstall": "webpack -p --config ./webpack.production.config.js --progress --colors",
    "production": "cross-env NODE_ENV=production webpack -p --config ./webpack.production.config.js",
    "sim": "cross-env NODE_ENV=production webpack -p --config ./webpack.production.config.js && NODE_ENV=production babel-node server.js"
  },
  "keywords": [
    "vegan"
  ],
  "author": "Sean Campbell",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-loader": "3.1.0",
    "babel": "6.3.13",
    "babel-cli": "6.3.17",
    "babel-core": "6.3.21",
    "babel-loader": "6.2.0",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "classnames": "2.2.1",
    "cross-env": "1.0.5",
    "css-loader": "0.23.1",
    "express": "4.13.3",
    "file-loader": "0.8.5",
    "history": "1.7.0",
    "immutable": "3.7.6",
    "material-ui": "0.13.4",
    "moment": "2.10.6",
    "node-sass": "3.4.2",
    "node-uuid": "1.4.7",
    "normalize.css": "3.0.3",
    "path": "0.12.7",
    "ramda": "0.18.0",
    "react": "0.14.3",
    "react-addons-css-transition-group": "0.14.3",
    "react-dom": "0.14.3",
    "react-redux": "4.0.4",
    "react-router": "1.0.3",
    "react-tap-event-plugin": "0.2.1",
    "react-toolbox": "0.14.0",
    "redux": "3.0.5",
    "redux-simple-router": "1.0.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.9"
  },
  "devDependencies": {
    "babel-eslint": "4.1.6",
    "babel-plugin-react-transform": "2.0.0-beta1",
    "chai": "3.4.1",
    "chai-immutable": "1.5.3",
    "eslint-plugin-react": "3.11.3",
    "jsdom": "7.2.1",
    "mocha": "2.3.4",
    "react-addons-test-utils": "0.14.3",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "rimraf": "2.5.0",
    "webpack-dev-middleware": "1.4.0",
    "webpack-dev-server": "1.14.0",
    "webpack-hot-middleware": "2.6.0"
  }
}
